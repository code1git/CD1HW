{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Appsettings": {
    //application settings
    "DemoUIOnStart": true,
    "ResultPath": "./Result",
    "ProductType": "OSID",
    //camera settings
    // IR mode IR ramp on, off, both
    "IRMode": "both",
    "IRTimesleep": 1000,
    "CamIdx": 0,
    "CameraBackEnd": "DSHOW",
    //OSID : max 1920, 1440
    "CameraCropBBox": "150,80,1750,1020",
    "CameraRotate": 0,
    "CameraCrop": false,
    // Wacom signpad
    "SignPadFont": "굴림체",
    // nec demo setting
    "NecDemoFilePath": "./nec_list.csv",
    "NecDemoFileType": "csv"
  }
}
